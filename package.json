{
  "name": "mypictures",
  "description": "Projet de gestion d'images",
  "author": "Alexandre Del√®gue",
  "license": "",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/larousso/mypictures"
  },
  "homepage": "",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run build\" \"npm run start-prod\"",
    "start-test": "better-npm-run start-test",
    "start-prod": "better-npm-run start-prod",
    "build": "webpack --verbose --colors --display-error-details --config webpack.config.prod.js",
    "postinstall": "webpack --display-error-details --config webpack.config.prod.js",
    "start-dev": "better-npm-run start-dev",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\""
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "DBLOCATION": "/data/db",
        "IMAGESPATH": "/data/pictures",
        "LOGPATH": "/logs",
        "PORT": 8080
      }
    },
    "start-test": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "DBLOCATION": "/Users/adelegue/Documents/idea/mypictures/tmp",
        "IMAGESPATH": "/Users/adelegue/Documents/idea/mypictures/tmp",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
